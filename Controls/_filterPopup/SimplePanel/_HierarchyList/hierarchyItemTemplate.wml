<ws:partial template="Controls/dropdown:ItemTemplate"
            scope="{{_options}}"
            multiSelect="{{itemData.getPropValue(itemData.item, nodeProperty) ? false : multiSelect}}"
            markerVisibility="{{itemData.getPropValue(itemData.item, nodeProperty) ? 'onactivated' : multiSelect ? 'hidden'}}"
            checkBoxAlign="right"
            class="{{itemData.getPropValue(itemData.item, nodeProperty) ? 'controls-SimplePanel-folderItem_theme_' + _options.theme}}">
</ws:partial>
