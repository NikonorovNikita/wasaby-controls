<ws:for data="index, item in itemsViewModel.getViewIterator()">
    <ws:partial
            template="{{item.getItemTemplate('wml!Controls/_propertyGrid/itemTemplate', groupTemplate)}}"
            getItemStyles="{{getItemStyles}}"
            theme="{{theme}}"
            collectionItem="{{item}}"
            item="{{item.getContents()}}"
            on:propertyValueChanged="_propertyValueChanged(item.getContents())"
            on:click="_groupClick(item)"/>
</ws:for>

