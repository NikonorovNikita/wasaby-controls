<ws:partial template="{{ _options.content }}"
            validationStatus="{{_getValidStatus(_contentActive)}}"
            scope="{{_options}}"
            attr:class="controls-ValidateContainer__validationStatus-{{_getValidStatus(_contentActive)}}"
            on:selectedKeysChanged="_selectedKeysChangedHandler()"
            on:selectedKeyChanged="_selectedKeyChangedHandler()"
            on:deactivated="_deactivatedHandler()"
            on:mouseenter="_hoverHandler()"
            on:focusIn="_focusInHandler()"
            on:mouseleave="_mouseLeaveHandler()"
/>
