<ws:for data="action in showedActions">
    <ws:partial
            template="wml!Controls/_list/ItemActions/resources/ItemAction"
            itemData="{{itemData}}"
            hasTitle="{{(!action.showTitle && !action.icon) || action.showTitle}}"
            hasIcon="{{(!action.showIcon && action.icon) || action.showIcon}}"
            tooltip="{{(action.tooltip || action.title)}}"
            size="{{size}}"
            action="{{action}}"/>
</ws:for>
